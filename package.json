{
    "name": "autheventissue",
    "description": "Used for testing auth provider",
    "version": "0.0.1",
    "license": "MIT",
    "publisher": "ms-azuretools",
    "main": "./extension.js",
    "browser": "./extension.js",
    "engines": {
        "vscode": "^1.105.0"
    },
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.signIn",
                "title": "AuthExtn: Sign in to new account (clearSessionPreference=true)"
            },
            {
                "command": "extension.getAccounts",
                "title": "AuthExtn: List accounts"
            },
            {
                "command": "extension.getSessionSilent",
                "title": "AuthExtn: Get session silently"
            },
            {
                "command": "extension.parallelGetSessions",
                "title": "AuthExtn: Test parallel silent getSession() API calls"
            }
        ]
    },
    "scripts": {
        "run-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
    },
    "devDependencies": {
        "@vscode/test-web": "^0.0.75"
    }
}
